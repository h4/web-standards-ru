---
layout: 'html'
---

<h1><%- @document.title %></h1>

<p>
	<time datetime="<%= @getDate(@document.date, 'YYYY-MM-DD') %>">
		<%= @getDate(@document.date) %>
	</time>
</p>

<iframe allowfullscreen src="//youtube.com/embed/<%- @document.youtube %>?controls=2&amp;modestbranding=1&amp;theme=light&amp;showinfo=0"></iframe>

<% for entry in @getRelatedDocuments(): %>
<ul>
	<li><a href="<%= entry.url %>"><%- entry.title %></a></li>
</ul>
<% end %>
